{
  "name": "me",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@fortawesome/fontawesome-pro-light": "^5.0.6",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/react-test-renderer": "^16.0.0",
    "express": "^4.16.2",
    "mixpanel-browser": "^2.17.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-github-corner": "^2.1.0",
    "react-helmet": "^5.2.0",
    "react-mixpanel": "^0.0.11",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.0",
    "shortid": "^2.2.8"
  },
  "scripts": {
    "build": "npm-run-all build:css build:js",
    "build:css": "node-sass-chokidar src/ -o src/",
    "build:js": "react-scripts build",
    "eject": "react-scripts eject",
    "postbuild": "node ./tools/convertMarkupToEJS && node ./tools/injectTemplateTags",
    "serve": "node ./server/bin/www",
    "start": "npm run start:dev",
    "start:dev": "npm-run-all -p watch:css start:js",
    "start:server": "npm-run-all -p watch:css start:js",
    "start:js": "react-scripts start",
    "pretest": "npm run build:css",
    "test": "react-scripts test --env=jsdom",
    "watch:css": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "bootstrap": "^4.0.0",
    "chalk": "^2.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "^0.0.3",
    "proxyquire": "^1.8.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.2.0",
    "reactstrap": "5.0.0-beta",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
